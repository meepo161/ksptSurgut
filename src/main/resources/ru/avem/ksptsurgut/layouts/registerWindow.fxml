<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="root" prefHeight="429.0" prefWidth="617.0" stylesheets="@../styles/main_css.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.avem.ksptsurgut.controllers.RegisterWindowController">
    <children>
      <VBox alignment="TOP_CENTER" layoutX="295.0" layoutY="14.0" spacing="16.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER">
               <children>
                    <Label text="Редактирование профиля" />
               </children>
            </HBox>
            <HBox spacing="16.0">
               <children>
                  <VBox>
                     <children>
                  
                          <TableView fx:id="tableProfiles" prefHeight="526.0" prefWidth="248.0">
                              <placeholder>
                                  <Label text="${'ОСТОРОЖНО!!! \nДобавьте хотя бы 1 профиль!'}" />
                              </placeholder>
                              <columns>
                                  <TableColumn fx:id="columnProfilesLogin" prefWidth="75.0" text="Логин" />
                                  <TableColumn fx:id="columnProfilesFullName" prefWidth="75.0" text="ФИО" />
                              </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                          </TableView>
                     </children>
                  </VBox>
                  <VBox layoutX="10.0" layoutY="10.0" spacing="8.0">
                     <children>
                        <HBox alignment="CENTER_RIGHT" spacing="8.0">
                           <children>
                        
                                <Label text="Логин" />
                                <TextField fx:id="textProfilesLogin" />
                           </children>
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" spacing="8.0">
                           <children>
                        
                                <Label text="Должность" />
                                <TextField fx:id="textProfilesPosition" />
                           </children>
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" spacing="8.0">
                           <children>
                        
                                <Label text="Табельный номер" />
                                <TextField fx:id="textProfilesPositionNumber" />
                           </children>
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" spacing="8.0">
                           <children>
                        
                                <Label text="ФИО" />
                                <TextField fx:id="textProfilesFullName" />
                           </children>
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" spacing="8.0">
                           <children>
                        
                                <Label text="Пароль" />
                                <PasswordField fx:id="textProfilesPassword1" />
                           </children>
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" spacing="8.0">
                           <children>
                        
                                <Label text="Ещё раз" />
                                <PasswordField fx:id="textProfilesPassword2" />
                           </children>
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER" spacing="8.0">
                           <children>
                        
                                <JFXButton buttonType="RAISED" mnemonicParsing="false" onAction="#handleProfilesDeleteProfile" text="Удалить" />
                                <JFXButton buttonType="RAISED" defaultButton="true" mnemonicParsing="false" onAction="#handleProfilesAddProfile" text="Добавить" />
                           </children>
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </HBox>
                     </children>
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
         </padding>
      </VBox>
    </children>
</AnchorPane>
